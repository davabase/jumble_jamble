global.map = ds_map_create();

for (var i = 0; i < array_length_1d(global.tiles); i++)
{
    var key = global.tiles[i];
    if (key != 0)
    {
        if (!ds_map_exists(global.map, key))
        {
            var list = ds_list_create();
            ds_list_add(list, i);
            ds_map_add(global.map, key, list);
        }
        else
        {
            var list = ds_map_find_value(global.map, key);
            ds_list_add(list, i);
        }
    }
}

